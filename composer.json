{
    "name": "jpereira/pexels",
    "description": "library to consume the pexels api (https://www.pexels.com/)",
    "version": "1.0.2.1",
    "require": {        
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jpereira\\Pexels\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jpereira\\Pexels\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Joaquin Pereira",
            "email": "pereira.joaquin@gmail.com"
        }
    ],
    "minimum-stability": "dev",    
    "require-dev": {
        "phpunit/phpunit": "^10.3@dev",
        "orchestra/testbench": "dev-develop"
    },
    "extra": {        
        "laravel": {
            "providers": [
                "Jpereira\\Pexels\\Providers\\PexelsServiceProvider"
            ]
        }
    }
}
